Title: <%= @post.title %><br>
Content: <%= @post.content %><br>

        <% if current_user == @post.user %>
        <a href="/posts/<%= @post.id %>/edit">Edit Post</a>

        <form action="/posts/<%= @post.id %>" method="post">
        <input type="hidden" name="_method" value="delete">
        </form>
        
        <a href="/posts"> Publish Your Post </a> 
        <% end %>
